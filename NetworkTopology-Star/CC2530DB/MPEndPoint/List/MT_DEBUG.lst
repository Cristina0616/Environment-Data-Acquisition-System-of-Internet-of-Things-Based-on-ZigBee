     65          uint8 MT_DebugCommandProcessing(uint8 *pBuf)
     66          {
     67            uint8 status = MT_RPC_SUCCESS;
     68          
     69            switch (pBuf[MT_RPC_POS_CMD1])
     70            {
     71              case MT_DEBUG_SET_THRESHOLD:
     72                MT_DebugSetThreshold(pBuf);
     73                break;
     74          
     75              default:
     76                status = MT_RPC_ERR_COMMAND_ID;
     77                break;
     78            }
     79          
     80            return status;
     81          }
